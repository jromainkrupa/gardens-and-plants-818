<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @garden.name %>
</p>

<p>
  <strong>Banner url:</strong>
  <%= @garden.banner_url %>
</p>

<ul>
<% @garden.plants.each do |plant| %>
  <li> <%= plant.name %></li>
<%= link_to 'destroy', plant_path(plant), method: :delete %>
<% end  %>
</ul>

<%= link_to 'Edit', edit_garden_path(@garden) %>
<%= link_to 'Back', gardens_path %>

<%= render "plants/form"  %>
